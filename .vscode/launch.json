{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run transcript_saver",
      "type": "debugpy",
      "request": "launch",

      "module": "notetaker.core.transcript_saver.transcript_saver",

      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "env": {
        "GOOGLE_APPLICATION_CREDENTIALS": "${workspaceFolder}/src/notetaker/core/google_storage/storage_API.json"
      },
      "justMyCode": true
    }
  ]
}
